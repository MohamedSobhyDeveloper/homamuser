<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.otex.homamuser">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Homamuser"
        android:usesCleartextTraffic="true"
        tools:ignore="AllowBackup">
        <activity android:name=".view.home.HomeActivity"></activity>
        <activity android:name=".view.contactus.ContactUsActivity" />
        <activity android:name=".view.aboutus.AboutUsActivity" />
        <activity android:name=".view.forgetpassword.ActivityForgetPassword" />
        <activity android:name=".view.register.RegisterActivity" />
        <activity android:name=".view.login.LoginActivity" />
        <activity android:name=".view.splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.MainActivity" />
    </application>

</manifest>